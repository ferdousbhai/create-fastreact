{
  "name": "create-fastreact",
  "version": "1.0.0",
  "description": "Create fastreact apps with React + FastAPI + Modal",
  "type": "module",
  "bin": {
    "create-fastreact": "./dist/index.js"
  },
  "files": [
    "dist",
    "template"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/create.ts --format esm --clean --dts",
    "dev": "tsup src/index.ts src/create.ts --format esm --watch --dts",
    "sync-version": "npm version $(git describe --tags --abbrev=0 | sed 's/^v//') --no-git-tag-version --allow-same-version",
    "prepare": "git config core.hooksPath .githooks"
  },
  "dependencies": {
    "open": "^11.0.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "react",
    "fastapi",
    "modal",
    "fullstack",
    "template",
    "create"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferdousbhai/create-fastreact.git"
  }
}
